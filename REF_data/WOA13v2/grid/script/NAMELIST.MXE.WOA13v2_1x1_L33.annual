  dy_const_deg          =   1.0d0,
/

&nml_topo
  file_topo             = "dummy.d",
/

&nml_record_date
  rec_first_date        = 1000, 1, 1, 0, 0, 0,
  rec_last_date         = 1000, 1, 1, 0, 0, 0,
  rec_interval_date     =    1, 0, 0, 0, 0, 0,
/
