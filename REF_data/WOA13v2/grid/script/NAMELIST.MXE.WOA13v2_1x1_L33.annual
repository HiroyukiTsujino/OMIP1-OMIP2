  imut                  = 360,
  jmut                  = 180,
  km                    = 32,
  ksgm                  = 32,
  kbbl			= 0,
/

&nml_coordinate
  lspherical            = .true.,
  lsub                  = .false.,
  lzstar                = .true.,
/

&nml_vert_grid
